{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "name": "restaurants",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slogan": {
      "type": "string"
    },
    "profile": {
      "model": "profiles"
    },
    "foods": {
      "via": "restaurant",
      "collection": "foods"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "restaurant"
    },
    "qrImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "via": "restaurant",
      "collection": "categories"
    },
    "banners": {
      "type": "component",
      "repeatable": true,
      "component": "common.slide"
    }
  }
}
